<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="LaunchTheme" parent="Theme.SplashScreen">
        <item name="android:windowBackground">@drawable/launch_background</item>
        <item name="android:forceDarkAllowed">false</item>
        <item name="android:windowFullscreen">false</item>
        <item name="android:windowDrawsSystemBarBackgrounds">false</item>
        <item name="android:windowLayoutInDisplayCutoutMode">shortEdges</item>
        <item name="windowSplashScreenBackground">#F7F5F0</item>
        <item name="windowSplashScreenAnimatedIcon">@drawable/android12splash</item>
        <item name="windowSplashScreenIconBackgroundColor">#F7F5F0</item>
        <item name="postSplashScreenTheme">@style/NormalTheme</item>
    </style>
    <style name="NormalTheme" parent="@android:style/Theme.Black.NoTitleBar">
        <item name="android:windowBackground">#F7F5F0</item>
    </style>
</resources>
